{
    "name": "Swot",
    "description": "The best way to learn new things",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons":{
        "16": "images/swot-logo.png",
        "32": "images/swot-logo.png",
        "48": "images/swot-logo.png",
        "128": "images/swot-logo.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "browser_action":{
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
     },
    "permissions":[
        "storage"
    ]

    
}